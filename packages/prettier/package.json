{
  "name": "@2digits/prettier-config",
  "version": "0.8.0",
  "license": "MIT",
  "public": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "prettier-config"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "prettier": ">=2.8.0"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "local-pkg": "^0.4.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prettier-plugin-toml": "^0.3.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@2digits/eslint-config": "*",
    "@2digits/tsconfig": "*",
    "@types/prettier": "^2.7.3",
    "eslint": ">=8.43.0",
    "prettier": "^2.8.8",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "eslintConfig": {
    "extends": "@2digits"
  }
}
