{
  "name": "@2digits/eslint-plugin",
  "version": "3.0.10",
  "description": "An eslint plugin that provides a set of rules to enforce best practices for 2digits projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/2digits-agency/configs.git",
    "directory": "packages/eslint-plugin"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "types": "tsc --noEmit",
    "test": "vitest --run"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "2digits"
  ],
  "license": "MIT",
  "public": true,
  "dependencies": {
    "@typescript-eslint/utils": "catalog:",
    "eslint": "catalog:",
    "ts-pattern": "catalog:"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "@typescript-eslint/parser": "catalog:",
    "eslint-vitest-rule-tester": "catalog:",
    "magic-regexp": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
