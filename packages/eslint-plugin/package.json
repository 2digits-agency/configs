{
  "name": "@2digits/eslint-plugin",
  "version": "2.3.21",
  "description": "An eslint plugin that provides a set of rules to enforce best practices for 2digits projects",
  "repository": {
    "url": "https://github.com/2digits-agency/configs",
    "directory": "packages/eslint-plugin"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "test": "vitest --run"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "2digits"
  ],
  "license": "MIT",
  "public": true,
  "dependencies": {
    "@typescript-eslint/utils": "8.19.1",
    "eslint": "9.17.0",
    "magic-regexp": "0.8.0",
    "ts-pattern": "5.6.0"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "@typescript-eslint/parser": "8.19.1",
    "eslint-vitest-rule-tester": "0.7.1",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vitest": "2.1.8"
  }
}
