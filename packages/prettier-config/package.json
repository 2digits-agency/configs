{
  "name": "@2digits/prettier-config",
  "version": "2.2.0",
  "type": "module",
  "license": "MIT",
  "public": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "prettier-config"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "prettier": ">=3.3.2"
  },
  "dependencies": {
    "@2digits/constants": "0.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "local-pkg": "^0.5.0",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prettier-plugin-toml": "^2.0.1"
  },
  "devDependencies": {
    "@2digits/tsconfig": "0.4.0",
    "eslint": "^9.5.0",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2"
  }
}
