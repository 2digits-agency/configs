{
  "name": "@2digits/prettier-config",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/2digits-agency/configs.git",
    "directory": "packages/prettier-config"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsdown --minify",
    "dev": "tsdown --watch",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "prettier-config"
  ],
  "license": "MIT",
  "public": true,
  "dependencies": {
    "@2digits/constants": "workspace:*",
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@prettier/plugin-oxc": "catalog:",
    "@prettier/plugin-xml": "catalog:",
    "local-pkg": "catalog:",
    "prettier-plugin-jsdoc": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "prettier": "3.6.0"
  }
}
