{
  "name": "@2digits/prettier-config",
  "version": "1.1.1",
  "license": "MIT",
  "public": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint --ignore-path ../../.gitignore .",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "prettier-config"
  ],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "prettier": ">=2.8.0"
  },
  "dependencies": {
    "@2digits/constants": "*",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "local-pkg": "^0.5.0",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "prettier-plugin-toml": "^2.0.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@2digits/eslint-config": "*",
    "@2digits/tsconfig": "*",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": "@2digits"
  }
}
