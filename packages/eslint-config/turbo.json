{
  "extends": ["//"],
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", "tsdown.config.ts", "!test/**", "!fixtures/**", "!vitest.config.ts"]
    },
    "docs": {
      "outputs": ["./.eslint-config-inspector/**"],
      "dependsOn": ["build", "^build"],
      "inputs": ["$TURBO_DEFAULT$", "$TURBO_ROOT$/eslint.config.ts"]
    },
    "docs:dev": {
      "dependsOn": ["build", "^build"],
      "persistent": true,
      "cache": false,
      "interactive": true
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["$TURBO_DEFAULT$", "vitest.config.ts"]
    }
  }
}
