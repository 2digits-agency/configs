{
  "name": "@2digits/eslint-config",
  "description": "Effortlessly enforce best practices and catch errors with this comprehensive ESLint configuration for TypeScript, featuring popular plugins like @typescript-eslint, eslint-plugin-react, and eslint-plugin-unicorn.",
  "version": "2.5.1",
  "type": "module",
  "license": "MIT",
  "public": true,
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@2digits/constants": "workspace:*",
    "@2digits/eslint-plugin": "workspace:*",
    "@eslint-react/eslint-plugin": "1.12.4",
    "@eslint/compat": "1.1.1",
    "@eslint/js": "9.9.1",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@next/eslint-plugin-next": "14.2.7",
    "@tanstack/eslint-plugin-query": "5.53.0",
    "@typescript-eslint/parser": "8.3.0",
    "eslint-config-flat-gitignore": "0.1.8",
    "eslint-config-prettier": "9.1.0",
    "eslint-flat-config-utils": "0.3.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-jsdoc": "50.2.2",
    "eslint-plugin-n": "17.10.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-compiler": "0.0.0-experimental-f8a5409-20240829",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-sonarjs": "1.0.4",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-tailwindcss": "3.17.4",
    "eslint-plugin-turbo": "2.1.0",
    "eslint-plugin-unicorn": "55.0.0",
    "find-up": "7.0.0",
    "globals": "15.9.0",
    "local-pkg": "0.5.0",
    "typescript-eslint": "8.3.0"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "@types/eslint": "9.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.16.2",
    "@typescript-eslint/utils": "8.3.0",
    "eslint": "9.9.1",
    "eslint-typegen": "0.3.1",
    "tsup": "8.2.4",
    "typescript": "5.5.4"
  }
}
