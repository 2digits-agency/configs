{
  "name": "@2digits/eslint-config",
  "version": "3.0.1",
  "description": "Effortlessly enforce best practices and catch errors with this comprehensive ESLint configuration for TypeScript, featuring popular plugins like @typescript-eslint, eslint-plugin-react, and eslint-plugin-unicorn.",
  "homepage": "https://2d-configs.vercel.app/",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "docs": "config-inspector build",
    "docs:dev": "config-inspector"
  },
  "keywords": [
    "eslint-config"
  ],
  "license": "MIT",
  "public": true,
  "dependencies": {
    "@2digits/constants": "workspace:*",
    "@2digits/eslint-plugin": "workspace:*",
    "@eslint-community/eslint-plugin-eslint-comments": "4.4.1",
    "@eslint-react/eslint-plugin": "1.26.2",
    "@eslint/compat": "1.2.6",
    "@eslint/core": "0.11.0",
    "@eslint/js": "9.20.0",
    "@eslint/plugin-kit": "0.2.6",
    "@graphql-eslint/eslint-plugin": "4.3.0",
    "@next/eslint-plugin-next": "15.1.7",
    "@tanstack/eslint-plugin-query": "5.66.1",
    "@typescript-eslint/parser": "8.24.1",
    "css-tree": "3.1.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-flat-config-utils": "2.0.1",
    "eslint-plugin-antfu": "3.1.0",
    "eslint-plugin-de-morgan": "1.1.0",
    "eslint-plugin-drizzle": "0.2.3",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-jsonc": "2.19.1",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-compiler": "19.0.0-beta-e552027-20250112",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-storybook": "0.11.3",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-turbo": "2.4.2",
    "eslint-plugin-unicorn": "57.0.0",
    "find-up": "7.0.0",
    "globals": "16.0.0",
    "graphql-config": "5.1.3",
    "jsonc-eslint-parser": "2.4.0",
    "local-pkg": "1.0.0",
    "typescript-eslint": "8.24.1"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "@eslint/config-inspector": "1.0.0",
    "@eslint/css": "0.4.0",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.13.4",
    "@typescript-eslint/utils": "8.24.1",
    "eslint": "9.20.1",
    "eslint-typegen": "2.0.0",
    "tsup": "8.3.6",
    "typescript": "5.7.3"
  }
}
