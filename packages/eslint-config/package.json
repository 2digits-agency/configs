    "graphql-config": "catalog:",
    "jsonc-eslint-parser": "catalog:",
    "local-pkg": "catalog:",
    "typescript-eslint": "catalog:",
    "yaml-eslint-parser": "catalog:"
  },
  "devDependencies": {
    "@2digits/tsconfig": "workspace:*",
    "@eslint/config-inspector": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "dedent": "catalog:",
    "eslint": "catalog:",
    "eslint-typegen": "catalog:",
    "execa": "catalog:",
    "react": "catalog:",
    "tinyglobby": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  }
